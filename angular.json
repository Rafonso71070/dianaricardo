{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "diana-ricardo": {
      "root": "",
      "sourceRoot": "docs",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/diana-ricardo",
            "index": "docs/index.html",
            "main": "docs/main.ts",
            "polyfills": "docs/polyfills.ts",
            "tsConfig": "docs/tsconfig.app.json",
            "assets": [
              "docs/favicon.ico",
              "docs/assets"
            ],
            "styles": [
              "docs/assets/css/glanz_library.css",
              "docs/assets/css/glanz_style.css",
              "docs/styles.css"
            ],
            "scripts": [
              "docs/assets/js/glanz_library.js",
              "docs/assets/js/glanz_script.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "docs/environments/environment.ts",
                  "with": "docs/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "diana-ricardo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "diana-ricardo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "diana-ricardo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/assets/css/glanz_library.css",
              "src/assets/css/glanz_style.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/glanz_library.js",
              "src/assets/js/glanz_script.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "diana-ricardo-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "diana-ricardo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "diana-ricardo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "diana-ricardo"
}